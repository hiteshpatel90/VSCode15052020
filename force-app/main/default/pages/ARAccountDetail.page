              function formatAddress(streetStr, cityStr, stateStr, zipStr, countryStr) {
    
                var address = "";
                var street = streetStr || "";
                var city = cityStr || "";
                var zip = zipStr || "";
                var state = stateStr || "";
                var country = countryStr || "";
    
                if ( street.length > 0 ) {
                  address = street+"<br/>";
                }
    
                if ( city.length > 0 ) {
                  address += city+", ";
                }
                address +=  state+" "+zip;
    
                if ( address.length > 0 ) { address += " "; }
                address += country;
    
                return address;
              }              
